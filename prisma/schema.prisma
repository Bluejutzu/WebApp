generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model users {
  id       String @id @default(uuid())
  username String @unique @db.VarChar(50)
  email    String @unique @db.VarChar(100)
  password String @db.VarChar(255)
}
